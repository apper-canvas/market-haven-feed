{
  "/": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Homepage is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/product/:id": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Product details are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/category/:categoryName": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Category pages are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/search": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Search is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/cart": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view cart",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/checkout": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to checkout",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/orders": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view orders",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/wishlist": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "User must be logged in to view wishlist",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login"
    }
  },
  "/login": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Login page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/signup": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Signup page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/callback": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Callback is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/error": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Error page is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/reset-password/:appId/:fields": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reset password is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  },
  "/prompt-password/:appId/:emailAddress/:provider": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Prompt password is public",
            "rule": "public"
          }
        ]
      },
      "excludeRedirectQuery": true
    }
  }
}